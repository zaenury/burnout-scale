{
  "name": "nuxt-app",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "yarn lint:ts && yarn lint:prettier",
    "lint:fix": "yarn lint:fix:ts && yarn lint:fix:prettier",
    "lint:prettier": "prettier --check .",
    "lint:fix:prettier": "prettier --write --list-different .",
    "lint:ts": "eslint --ext \".ts,.vue\" --ignore-path .eslintignore .",
    "lint:fix:ts": "eslint --ext \".ts,.vue\" --ignore-path .eslintignore . --fix"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nuxt": "^3.8.0",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "vue": "^3.3.6",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@mdi/font": "^7.3.67",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "animate.css": "^4.1.1",
    "sass": "^1.69.4",
    "standard": "^17.1.0",
    "typescript": "^5.2.2",
    "vue-eslint-parser": "^9.3.2",
    "vuetify": "^3.4.0-alpha.1"
  }
}
